          <colspec colname="c3" colnum="3" align="right"/>
          <thead>
            <row>
              <entry>
                <emphasis>Particle</emphasis>
              </entry>
              <entry nameend="c3" namest="c2">
                <emphasis>Quark Composition</emphasis>
              </entry>
            </row>
          </thead>
          <tbody>
            <row>
              <entry nameend="c3" namest="c1">
                <emphasis>
                  <emphasis effect="italics">Mesons</emphasis>
                </emphasis>
              </entry>
            </row>
            <row>
              <entry nameend="c2" namest="c1">0</entry>
              <entry>1</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
      <table>
